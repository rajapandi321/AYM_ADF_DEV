					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "PostgreSqlV2Source",
						"query": "select \n\tssh.id,\n\tssh.order_id,\n\tCASE \n\t\tWHEN ssh.state='shipped' THEN 'delivered'\n\t\tWHEN ssh.state='pending' THEN 'pending'\n\t\tWHEN ssh.state='ready' THEN 'picked'\n\t\tWHEN ssh.state='canceled' THEN 'cancelled'\n\tEND as status,\n\tssh.created_at,\n\tssh.updated_at,\n\tCAST((ssh.additional_tax_total + ssh.included_tax_total) AS DOUBLE PRECISION) AS tax_total,\n\tso.total,\n\tso.order_type,\n\tslto.time_slots_schedule_id,\n\tso.supermarket_id,\n\tsla.delivery_date,\n\tsa.amount as delivery_charges,\n\tfalse as is_delivery_on_hold,\n\tcurrent_timestamp as processing_start_at\nfrom spree_shipments ssh\nleft join spree_orders so on so.id = ssh.order_id and so.state!='cart'\nleft join spree_lift_assignments sla on sla.order_id = ssh.order_id\nleft join spree_adjustments sa on sa.order_id = ssh.order_id and label='Express Fee'\nleft join danube_slot_to_orders slto on slto.order_id = ssh.order_id\norder by id asc;"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
